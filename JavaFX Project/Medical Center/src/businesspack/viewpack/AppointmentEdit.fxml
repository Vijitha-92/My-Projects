<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0"
            xmlns="http://javafx.com/javafx/8.0.172-ea" fx:controller="businesspack.viewpack.AppointmentEdit">
    <children>
        <GridPane layoutX="72.0" layoutY="48.0" prefHeight="191.0" prefWidth="389.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="Doctor Name" GridPane.rowIndex="3"/>
                <Label text="Appointment Date"/>
                <Label text="Patient Name" GridPane.rowIndex="4"/>
                <Label text="Appointment End  Time" GridPane.rowIndex="2"/>
                <DatePicker fx:id="appDate" prefHeight="40.0" prefWidth="205.0" GridPane.columnIndex="1"/>
                <ChoiceBox fx:id="timecheck" prefHeight="39.0" prefWidth="199.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="3"/>
                <ChoiceBox fx:id="endtime" prefHeight="50.0" prefWidth="194.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="2"/>
                <ChoiceBox fx:id="doctorChoice" prefHeight="44.0" prefWidth="194.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="3"/>
                <ChoiceBox fx:id="patientChoice" prefHeight="47.0" prefWidth="194.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="4"/>
                <Label text="Appointment Start Time" GridPane.rowIndex="1"/>
                <ChoiceBox fx:id="starttime" prefHeight="34.0" prefWidth="194.0" GridPane.columnIndex="1"
                           GridPane.rowIndex="1"/>
            </children>
        </GridPane>
        <ButtonBar layoutX="167.0" layoutY="292.0" prefHeight="40.0" prefWidth="200.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#handleOk" text="Ok"/>
                <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel"/>
            </buttons>
        </ButtonBar>
    </children>
</AnchorPane>
